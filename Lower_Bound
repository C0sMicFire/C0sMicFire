#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int n,a[10],b[10],x;
    cin>>n;
    for (int i=0; i<n; i++) {
    cin>>a[i];
    } 
    cin>>x;
    for (int i=0; i<x; i++) {
    cin>>b[i];
    }
    for(int i=0;i<x;i++)
    {   
        for (int j=0; j<n; j++) {
            if(a[j]==b[i])
                {cout<<"Yes "<<j+1<<endl;
                    break;
                }
            else if(a[j]>b[i])
         {       
                cout<<"No "<<j+1<<endl;
                break; 
        }     
        
        }
    }
    return 0;
}
